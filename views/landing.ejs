<section class="section section--projects" id="projects">
	<div class="wrapper">
		<h3 class="heading__subtitle u-margin-bottom-tiny">Latest Projects</h3>
		<h2 class="heading u-margin-bottom-xsmall">My work</h2>
		<div class="projects">
			<div class="owl-projects owl-carousel owl-theme">
				<% projects.forEach(function(project){ %>
				<% var i = 0; %>
				<%project.comments.forEach(function(comment){  %>
				<% i++ %>
				<% }); %>
				<div class="projects__item item">
					<div class="projects__item__image">
						<div class="projects__item__image__info">
							<div class="projects__item__category text text--tiny text--white">
								<%= project.category %>
							</div>
							<div class="projects__item__date text text--tiny text--white">
								<%= project.date.getDate() + "/" + (project.date.getMonth() + 1) + "/" + (project.date.getYear() + 1900) %>
							</div>
						</div>
						<% if(project.image == "") { %>
						<a href="/portfolio/<%= project._id %>" alt="test"><img
								src="/assets/images/placeholder-image.png" alt="placeholder image"></a>
						<% } else { %>
						<a href="/portfolio/<%= project._id %>" alt="test"><img src="<%= project.image %>"
								alt="project image"></a>
						<% } %>
					</div>
					<div class="projects__item__content">
						<h3 class="projects__item__title heading__subtitle heading__subtitle--white"><a
								class="text--white" href="/portfolio/<%= project._id %>"><%= project.name %></a></h3>
						<p class="projects__item__description text text--small text--white"><%= project.description %>
						</p>
					</div>
					<div class="projects__item__footer text text--tiny text--white">
						<div class="projects__item__likes">Likes: <%= project.likes.length %></div>
						<div class="projects__item__views">Views: <%= project.views %></div>
						<div class="projects__item__comments">Comments: <%= i %></div>
					</div>
				</div>

				<%  }); %>

			</div>

		</div>
	</div>
</section>

