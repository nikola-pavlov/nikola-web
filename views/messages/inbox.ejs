 <section class="section section--inbox">
 	<div class="wrapper">
 		<div class="inbox">
 			<div class="inbox__container">
 				<div class="inbox__control-panel">Control Panel</div>
 				<div class="inbox__message-panel">
 					Message Panel
 					<% messages.forEach(function(message){ %>
 					<div class="inbox__message">
 						<div class="inbox__message__icon"></div>
 						<div class="inbox__message__name">
 							<p class=""><strong>From:</strong> <%= message.name %></p>
 						</div>
 						<div class="inbox__message__email"><p class=""><strong>Email:</strong> <%= message.email %></p></div>
 						<div class="inbox__message__recieve-date"><p class=""><strong>Recieve Date</strong> <%= message.sendAt.getDate() + "." +  message.sendAt.getMonth() + "." +message.sendAt.getFullYear() %></p></div>
 						<div class="inbox__message__message-preview"><p class=""><%= message.message %></p></div>
 						
 						
 					</div>
 					<% }); %>
 					<div class="inbox__message">
 						Message
 					</div>
 					<div class="inbox__message">
 						Message
 					</div>
